00c0 ---- 50e5                         ld          R5, #229
00c1 ---- 68b1                         ld          R6, [#i2cTempAndHumiditySensor/state/i2cStatus]
00c2 ---- 70c6                         ld          R7, #/id0085
00c3 ---- 7cdd                         st          R7, [#i2cDeviceCommonReturn]
00c4 ---- 7080                         ld          R7, #(128 | 0)
00c5 ---- 0530                         jmp         I2cDeviceReadReg16MsbFirst
               /id0085:
00c6 ---- 08da                         ld          R0, [#i2cDeviceCommonBackupR0]
00c7 ---- 6cb1                         st          R6, [#i2cTempAndHumiditySensor/state/i2cStatus]
00c8 ---- 7caf                         st          R7, [#i2cTempAndHumiditySensor/output/hum]
               ;? i2cDeviceReadReg16MsbFirst(SI7021_I2C_ADDR, 0xE0; output.temp);
00c9 ---- 50e0                         ld          R5, #224
00ca ---- 68b1                         ld          R6, [#i2cTempAndHumiditySensor/state/i2cStatus]
00cb ---- 70cf                         ld          R7, #/id0091
00cc ---- 7cdd                         st          R7, [#i2cDeviceCommonReturn]
00cd ---- 7080                         ld          R7, #(128 | 0)
00ce ---- 0530                         jmp         I2cDeviceReadReg16MsbFirst
               /id0091:
00cf ---- 08da                         ld          R0, [#i2cDeviceCommonBackupR0]
00d0 ---- 6cb1                         st          R6, [#i2cTempAndHumiditySensor/state/i2cStatus]
00d1 ---- 7cb0                         st          R7, [#i2cTempAndHumiditySensor/output/temp]
               ;?
               ;? //gpioClearOutput(AUXIO_O_LED);
               ;?
