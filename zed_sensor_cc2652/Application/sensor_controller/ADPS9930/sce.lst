006b ---- 0000                         ld          R0, #0
006c ---- 0ca2                         st          R0, [#fwCtrlInt/taskId]
006d ---- 0002                         ld          R0, #(1 << 1)
006e ---- 0ca3                         st          R0, [#fwCtrlInt/bvActiveEvent]

