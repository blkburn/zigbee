00d2 ---- 08a5                         ld          R0, [#fwCtrlInt/bvTaskIoAlert]
00d3 ---- 8201                         or          R0, #(1 << 0)
00d4 ---- 0ca5                         st          R0, [#fwCtrlInt/bvTaskIoAlert]
               ;? fwScheduleTask(1);
00d5 ---- 0001                         ld          R0, #1
00d6 ---- 0c9f                         st          R0, [#(pFwTaskExecuteScheduleTable + 0)]
               ;?
               ;?
               i2cTempAndHumiditySensor/executeDone:
