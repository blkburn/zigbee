00bc ---- 0001                         ld          R0, #1
00bd ---- 0c9f                         st          R0, [#(pFwTaskExecuteScheduleTable + 0)]
               i2cTempAndHumiditySensor/initializeDone:
00be ---- adb7                         rts




               i2cTempAndHumiditySensor/execute:
                                       ; Start in low_power mode
00bf ---- 1499                         jsr         PwrRequestAndWaitForLowPowerMode

               ;? //gpioSetOutput(AUXIO_O_LED);
               ;?
