               .segment begin "Task: I2C Temp and Humidity Sensor"
               i2cTempAndHumiditySensor/initialize:
                                       ; Start in low_power mode
00b2 ---- 1499                         jsr         PwrRequestAndWaitForLowPowerMode

               ;?
               ;? i2cDeviceWriteReg8(SI7021_I2C_ADDR, 0xE6, 0xBB);
00b3 ---- 50e6                         ld          R5, #230
00b4 ---- 40bb                         ld          R4, #187
00b5 ---- 68b1                         ld          R6, [#i2cTempAndHumiditySensor/state/i2cStatus]
00b6 ---- 70ba                         ld          R7, #/id0078
00b7 ---- 7cdd                         st          R7, [#i2cDeviceCommonReturn]
00b8 ---- 7080                         ld          R7, #(128 | 0)
00b9 ---- 04de                         jmp         I2cDeviceWriteReg8
               /id0078:
00ba ---- 08da                         ld          R0, [#i2cDeviceCommonBackupR0]
00bb ---- 6cb1                         st          R6, [#i2cTempAndHumiditySensor/state/i2cStatus]
               ;?
               ;? //gpioCfgMode(AUXIO_O_LED,GPIO_MODE_OUTPUT);
               ;? //gpioClearOutput(AUXIO_O_LED);
